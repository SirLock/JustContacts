<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


 <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="260.0" minWidth="520.0" style="-fx-background-color: lightgrey;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContactDetailsController">
     <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="50.0" spacing="15.0" VBox.vgrow="ALWAYS">
         <children>
             <HBox fx:id="contactDetailsHBox" alignment="CENTER_LEFT" prefWidth="215.0">
                 <Label maxWidth="150.0" minWidth="150.0" text="Given Name">
                     <HBox.margin>
                         <Insets right="15.0" />
                     </HBox.margin>
                 </Label>
                 <TextField fx:id="givenNameInput" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                         <Insets />
                     </HBox.margin>
                 </TextField>
             </HBox>
             <HBox alignment="CENTER_LEFT" prefWidth="215.0">
                 <Label maxWidth="150.0" minWidth="150.0" text="Surname">
                     <HBox.margin>
                         <Insets right="15.0" />
                     </HBox.margin>
                 </Label>
                 <TextField fx:id="surnameInput" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                         <Insets />
                     </HBox.margin>
                 </TextField>
             </HBox>
             <HBox alignment="CENTER_LEFT" prefWidth="215.0">
                 <Label maxWidth="150.0" minWidth="150.0" text="Phone">
                     <HBox.margin>
                         <Insets right="15.0" />
                     </HBox.margin>
                 </Label>
                 <TextField fx:id="phoneInput" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                         <Insets />
                     </HBox.margin>
                 </TextField>
             </HBox>
             <HBox alignment="CENTER_LEFT" prefWidth="215.0">
                 <Label maxWidth="150.0" minWidth="150.0" text="Address">
                     <HBox.margin>
                         <Insets right="15.0" />
                     </HBox.margin>
                 </Label>
                 <TextField fx:id="addressInput" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                         <Insets />
                     </HBox.margin>
                 </TextField>
             </HBox>
         </children>
         <VBox.margin>
             <Insets />
         </VBox.margin>
     </VBox>
     <HBox alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="SOMETIMES">
         <children>
             <Label fx:id="infoLabel" alignment="BOTTOM_LEFT">
                 <font>
                     <Font size="14.0" />
                 </font>
             </Label>
             <Pane HBox.hgrow="ALWAYS" />
             <Button fx:id="editSaveButton" minHeight="30.0" minWidth="56.0" mnemonicParsing="false" onAction="#toggleEdit" text="edit" HBox.hgrow="ALWAYS" />
         </children>
     </HBox>
     <padding>
         <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
     </padding>
 </VBox>
